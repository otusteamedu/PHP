{
    "name": "dmitry_silinsky/sockets",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-sockets": ">=7.1.0",
        "ext-mbstring": ">=7.1.0",
        "ext-readline": ">=7.1.0",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
