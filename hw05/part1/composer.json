{
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.3",
    "pimple/pimple": "^3.2",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-diactoros": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "process-timeout": 0
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
