# название окружения приложения
env=<default: production>

# параметры подключения к харнилищу MongoDb
mongo_db_url=<default: localhost>
mongo_db_name=<default: local>

# параметры подключения к харнилищу Redis
redis_host=<default: localhost>
redis_port=<default: 6379>
redis_db=<default: 0>

# параметры Youtube
youtube_api_key=<key for Youtube API>